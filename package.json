{
  "name": "svelte-universal-component-compiler",
  "version": "0.0.1",
  "description": "Svelte compiler wrapper that outputs drop-in components that work in common UI frameworks without any configuration.",
  "main": "build.js",
  "bin": {
    "svelte-ucc": "build.js"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "eslint *.js && cd tests && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pospi/svelte-universal-component-compiler.git"
  },
  "keywords": [
    "Svelte",
    "compiler",
    "component",
    "WebComponent",
    "build-system",
    "React",
    "Angular",
    "Vue"
  ],
  "author": "pospi",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pospi/svelte-universal-component-compiler/issues"
  },
  "homepage": "https://github.com/pospi/svelte-universal-component-compiler#readme",
  "dependencies": {
    "ejs": "^3.1.5",
    "globby": "^10.0.1",
    "mkdirp-promise": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "svelte": "3.16.5"
  },
  "peerDependencies": {
    "svelte": "^3.16.5"
  }
